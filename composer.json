{
    "name": "arunov/online-shopping-cart-laravel",
    "description": "Online Shop Cart Laravel",
    "type": "library",
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^10.0|^11.0",
        "illuminate/session": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.1|^11.5",
        "mockery/mockery": "^1.4|^1.6",
        "orchestra/testbench": "^7|^9.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arunov\\OnlineShoppingCartLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arunov\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ovarun/online-shopping-cart-laravel"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Arunov\\OnlineShoppingCartLaravel\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Arunov\\OnlineShoppingCartLaravel\\Facades\\Cart"
            }
        }
    },
    "authors": [
        {
            "name": "O V",
            "email": "arunov8545@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
